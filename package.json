    {
      "path": "dist/icdev-productivity-core.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^11.0.1",
    "@types/uuid": "^9.0.7",
    "husky": "^8.0.3",
    "size-limit": "^11.0.1",
    "tsdx": "^0.13.3",
    "tslib": "^2.6.2",
    "typescript": "^3.9.10"
  },
  "dependencies": {
    "uuid": "^9.0.1"
  }
